#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_9d939d2b438026f1c52b9a50030c5e54/node_modules/vitepress/bin/node_modules:/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_9d939d2b438026f1c52b9a50030c5e54/node_modules/vitepress/node_modules:/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_9d939d2b438026f1c52b9a50030c5e54/node_modules:/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_9d939d2b438026f1c52b9a50030c5e54/node_modules/vitepress/bin/node_modules:/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_9d939d2b438026f1c52b9a50030c5e54/node_modules/vitepress/node_modules:/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_9d939d2b438026f1c52b9a50030c5e54/node_modules:/proc/cygdrive/c/Users/user/Documents/Projects/uchix/docs-new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
